package flowControlStaements;


//while loop
public class WhileLoop {

	public static void main(String[] args) {

		int i=0;
		while(i<=10) {
		
			
			System.out.println(++i);
		}
	}

}

//do while loop

 class DoWhileLoop{
	 public static void main(String[] args) {
		
		 int i=0;
		 do{
			 System.out.println("Inside do block");
		 }
		 while(i<=10) {
			 
		 }
			 
	}
	
}
