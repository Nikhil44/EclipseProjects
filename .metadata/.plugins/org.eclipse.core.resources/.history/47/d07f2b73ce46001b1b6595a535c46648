package encapsulation;

public class Test {

	public static void main(String[] args) {
		Customer c = new Customer();
		c.setfirstName("Nikhil");
		c.setLastName("Funde");
		c.setCreditcard("12346679");
		
		System.out.println("First Name "+c.getfirstName());
		c.getLastName();
		c.getCreditcard();
		}

}
