package stringbasic;

public class StringComparision {

	public static void main(String[] args) {
String s1="Niks";
String s2= "Niks";
String s3= "Tanu";
String s4= new String("Niks");

System.out.println(s1==s2);
System.out.println(s1==s3);
System.out.println(s1.equals(s2));
System.out.println();

	}

}
